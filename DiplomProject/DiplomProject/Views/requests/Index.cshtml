@model IEnumerable<DiplomProject.Models.requests>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.requestdesc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.requestfiles)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.requeststatus.statusname)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.requestdesc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.requestfiles)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.requeststatus.statusname)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.requestid }) |
            @Html.ActionLink("Details", "Details", new { id=item.requestid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.requestid })
        </td>
    </tr>
}

</table>
